@model SocialNetwork

<div class="">
    <div class="text-zinc-800 text-lg mb-4 font-semibold">
        جزئیات شبکه اجتماعی:
    </div>
    <!-- product -->
    <div class="mt-7 flex flex-col md:flex-row border-b pb-4">

        <div class="mr-2 md:mr-5 w-full">

            <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlTellgram"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlTellgram
            </div>

            <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlInstagram"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlInstagram
            </div>

            <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlWhatsUp"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlWhatsUp
            </div>

            <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlYoutube"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlYoutube
            </div>


            <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlAparat"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlAparat
            </div>

             <div class="text-xs sm:text-sm text-zinc-700">
                <label class="text-yellow-500" asp-for="UrlEita"></label><span class="text-yellow-500">:</span><br />
                @Model.UrlEita
            </div>

        </div>
    </div>
    <a asp-area="Admin" asp-action="Index" asp-controller="SocialNetworks" class="mx-auto w-1/3 px-2 py-3 mt-8 text-sm bg-red-500 hover:bg-red-400 transition text-gray-100 rounded-lg">بازگشت</a>

</div>
